<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>bpReadingModule</id>
	<name>bpReadingModule Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.bpReadingModule</package>
	<author>rakshit</author>
	<description>
		Helps to start writing OpenMRS modules
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/bpReadingModule/update.rdf</updateURL>
	<require_version>1.8.2</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.bpReadingModule.BpReadingModuleActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.bpReadingModule.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		bpReadingModule.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with bpReadingModule. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

